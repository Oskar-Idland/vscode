// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+l",
        "command": "extension.multiCommand.execute",
        "args": {
            "sequence": [
                "cSpell.goToPreviousSpellingIssue",
                {
                    "command": "editor.action.codeAction",
                    "args": {
                        "kind": "quickfix",
                        "apply": "first"
                    }
                },
                "cursorUndo",
            ]
        }
    },
    {
        "key": "ctrl+j",
        "command": "extension.jumpy-word"
    },
    {
        "key": "ctrl+l ctrl+j",
        "command": "extension.jumpy-line"
    },
    {
        "key": "ctrl+shift+r",
        "command": "workbench.action.reloadWindow"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.reloadWindow",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+shift+[Comma]",
        "command": "workbench.action.openSettingsJson"
    },
    // {
    //    "key": "shift+enter",
    //    "command": "latex-workshop.build",
    //    "when": "!config.latex-workshop.bind.altKeymap.enabled && !virtualWorkspace && editorLangId =~ /^latex$|^latex-expl3$|^doctex$|^rsweave$|^jlweave$|^pweave$/"
    // },
    {
        "key": "ctrl+alt+b",
        "command": "-latex-workshop.build",
        "when": "!config.latex-workshop.bind.altKeymap.enabled && !virtualWorkspace && editorLangId =~ /^latex$|^latex-expl3$|^rsweave$|^jlweave$|^pweave$/"
    },
    {
        "command": "runCommands",
        "key": "shift+enter",
        "args": {
            "commands": [
                "editor.action.formatDocument",
                "latex-workshop.build"
            ],
            "when": "editorTextFocus && !editorReadonly && !inCompositeEditor && !notebookEditorFocused && !findInputFocussed && !replaceInputFocussed && editorLangId =~ /^latex$|^latex-expl3$|^doctex$|^rsweave$|^jlweave$|^pweave$"
        },
        "when": "editorFocus && !findInputFocussed"
    },
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.files.openFolder",
        "when": "openFolderWorkspaceSupport"
    },
    {
        "key": "ctrl+k ctrl+o",
        "command": "-workbench.action.files.openFolder",
        "when": "openFolderWorkspaceSupport"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.toggleColumnSelection"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.editor.changeLanguageMode",
        "when": "!notebookEditorFocused"
    },
    {
        "key": "ctrl+k m",
        "command": "-workbench.action.editor.changeLanguageMode",
        "when": "!notebookEditorFocused"
    },
    {
        "key": "shift+tab",
        "command": "editor.action.inlineSuggest.commit",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus && !suggestWidgetVisible"
    },
    {
        "key": "tab",
        "command": "-editor.action.inlineSuggest.commit",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus && !suggestWidgetVisible"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+alt+left",
        "command": "hjump.jumpLeft"
    },
    {
        "key": "ctrl+alt+right",
        "command": "hjump.jumpRight"
    },
    {
        "key": "ctrl+alt+left",
        "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "ctrl+shift+alt+left",
        "command": "hjump.selectLeft"
    },
    {
        "key": "ctrl+shift+alt+right",
        "command": "hjump.selectRight"
    },
    {
        "key": "ctrl+[Minus]",
        "command": "editor.action.fontZoomIn"
    },
    {
        "key": "ctrl+-",
        "command": "editor.action.fontZoomOut"
    },
    {
        "key": "ctrl+-",
        "command": "-responsiveFonts.fontZoomOut",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+'",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+7",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+p",
        "command": "workbench.profiles.actions.switchProfile"
    },
    {
        "key": "alt+right",
        "command": "cursorLineEnd"
    },
    {
        "key": "shift+alt+right",
        "command": "cursorLineEndSelect"
    },
    {
        "key": "shift+alt+left",
        "command": "cursorLineStartSelect"
    },
    {
        "key": "alt+left",
        "command": "cursorLineStart"
    },
    {
        "key": "ctrl+alt+z",
        "command": "workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "ctrl+k z",
        "command": "-workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "ctrl+[Semicolon]",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+shift+[Equal]",
        "command": "-workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+shift+v",
        "command": "latex-utilities.formattedPaste",
        "when": "config.latex-utilities.formattedPaste.useAsDefault && editorTextFocus && editorLangId == 'latex'"
    },
    {
        "key": "ctrl+v",
        "command": "-latex-utilities.formattedPaste",
        "when": "config.latex-utilities.formattedPaste.useAsDefault && editorTextFocus && editorLangId == 'latex'"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+d",
        "command": "editor.action.duplicateSelection"
    },
    {
        "key": "ctrl+shift+enter",
        "command": "editor.action.selectAllMatches",
        "when": "editorFocus && findWidgetVisible"
    },
    {
        "key": "alt+enter",
        "command": "-editor.action.selectAllMatches",
        "when": "editorFocus && findWidgetVisible"
    },
    {
        "key": "ctrl+m",
        "command": "cSpell.goToPreviousSpellingIssueAndSuggest"
    },
    {
        "key": "alt+c",
        "command": "conceal.togglePrettySymbols",
        "when": "editorFocus"
    },
    {
        "key": "alt+c",
        "command": "-toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.view.debug",
        "when": "viewContainer.workbench.view.debug.enabled"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.moveSelectionToPreviousFindMatch"
    },
    {
        "key": "ctrl+shift+[Minus]",
        "command": "workbench.action.zoomIn"
    },
    {
        "key": "ctrl+[Minus]",
        "command": "-workbench.action.zoomIn"
    },
    {
        "key": "ctrl+shift+-",
        "command": "workbench.action.zoomOut"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.zoomOut"
    },
    {
        "key": "alt+n",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+g ctrl+b",
        "command": "git.checkout",
        "when": "!operationInProgress"
    },
    {
        "key": "ctrl+g ctrl+m",
        "command": "git.merge",
        "when": "!operationInProgress"
    },
    {
        "key": "ctrl+alt+v",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+g ctrl+f",
        "command": "git.fetch",
        "when": "!operationInProgress"
    },
    {
        "key": "shift+enter",
        "command": "-python.execSelectionInTerminal",
        "when": "editorTextFocus && !findInputFocussed && !isCompositeNotebook && !jupyter.ownsSelection && !notebookEditorFocused && !replaceInputFocussed && editorLangId == 'python'"
    },
    {
        "key": "ctrl+alt+d",
        "command": "editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "-editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "alt+p",
        "command": "umath.preview.toggleMathPreview"
    }
]