[
    {
        "key": "ctrl+l",
            "command": "extension.multiCommand.execute",
            "args": {
            "sequence": [
                "cSpell.goToPreviousSpellingIssue",
                {
                    "command": "editor.action.codeAction",
                    "args": {
                        "kind": "quickfix",
                        "apply": "first"
                    }
                },
                "cursorUndo",
            ]
        }
    },
    
    {
        "key": "alt+right",
        "command": "cursorWordPartRight",
        "when": "editorTextFocus",
    },
    
    {
        "key": "ctrl+k",
        "command": "extension.multiCommand.execute",
        "args": {
            "sequence": [
                "editor.action.marker.prev",
                {
                    "command": "editor.action.codeAction",
                    "args": {
                        "kind": "quickfix",
                        "apply": "first"
                    }
                },
                "cursorUndo",
            ]
        }
     },
    
