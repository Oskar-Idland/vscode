// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+shift+r",
        "command": "workbench.action.reloadWindow"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.reloadWindow",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.editor.changeLanguageMode",
        "when": "!notebookEditorFocused"
    },
    {
        "key": "ctrl+k m",
        "command": "-workbench.action.editor.changeLanguageMode",
        "when": "!notebookEditorFocused"
    },
    {
        "key": "shift+enter",
        "command": "runCommands",
        "args": {
            "commands": [
                {
                    "command": "terminal.focus",
                    "args": {
                        "languageId": ".c"
                    }
                },
                {
                    "command": "workbench.action.terminal.sendSequence",
                    "args": {
                        "text": "make\r",
                        "languageId": ".c"
                    },
                } 
            ],
            "text": "make\r"
        },
        "when": "editorFocus || terminalFocus"
    },
    // {
    //     "key": "shift+enter",
    //     "command": "workbench.action.terminal.sendSequence",
    //     "args": {
    //         "text": "make\r"
    //     },
    //     "when": "editorFocus || terminalFocus",
    // },
    // {
    //     "key": "shift+enter",
    //     "command": "workbench.action.terminal.sendSequence",
    //     "args": {
    //         "text": "gcc ${relativeFile} -Wall -o ${relativeFileDirname}/${fileBasenameNoExtension}.o\n${relativeFileDirname}/${fileBasenameNoExtension}.o\n"
    //     },
    //     "when": "editorFocus || terminalFocus",
    // },
    // {
    //     "key": "shift+enter",
    //     "command": "code-runner.run"
    // },
    {
        "key": "ctrl+j ctrl+l",
        "command": "extension.jumpy-line"
    },
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.files.openFolder",
        "when": "openFolderWorkspaceSupport"
    },
    {
        "key": "ctrl+k ctrl+o",
        "command": "-workbench.action.files.openFolder",
        "when": "openFolderWorkspaceSupport"
    },
    {
        "key": "ctrl+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.toggleColumnSelection",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+alt+oem_comma",
        "command": "extension.toggleSemicolon"
    },
    {
        "key": "ctrl+j ctrl+k",
        "command": "jump-extension.jump-to-the-end-of-a-word",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+q alt+w",
        "command": "-jump-extension.jump-to-the-end-of-a-word",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+j ctrl+j",
        "command": "jump-extension.jump-to-the-start-of-a-word",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+q alt+q",
        "command": "-jump-extension.jump-to-the-start-of-a-word",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+[Comma]",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "shift+tab",
        "command": "editor.action.inlineSuggest.commit",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus && !suggestWidgetVisible"
    },
    {
        "key": "tab",
        "command": "-editor.action.inlineSuggest.commit",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus && !suggestWidgetVisible"
    },
    {
        "key": "shift+enter",
        "command": "code-runner.run",
        "when": "!notebookEditorFocused && editorLangId == 'python'"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.moveSelectionToPreviousFindMatch"
    },
    {
        "key": "alt+enter",
        "command": "editor.action.previousMatchFindAction",
        "when": "editorFocus && findInputFocussed"
    },
    {
        "key": "shift+enter",
        "command": "-editor.action.previousMatchFindAction",
        "when": "editorFocus && findInputFocussed"
    },
    {
        "key": "ctrl+alt+p",
        "command": "workbench.profiles.actions.switchProfile"
    },
    {
        "key": "ctrl+alt+n",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+[Semicolon]",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+shift+[Equal]",
        "command": "-workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+'",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+7",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+a",
        "command": "-editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+'",
        "command": "-editor.togglequotes"
    },
    {
        "key": "ctrl+o",
        "command": "select-base-scopes.select",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+m",
        "command": "-select-base-scopes.select",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+-",
        "command": "editor.action.fontZoomOut"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "workbench.action.zoomOut"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.zoomOut"
    },
    {
        "key": "ctrl+shift+[Minus]",
        "command": "workbench.action.zoomIn"
    },
    {
        "key": "ctrl+[Minus]",
        "command": "-workbench.action.zoomIn"
    },
    {
        "key": "ctrl+[Minus]",
        "command": "editor.action.fontZoomIn"
    },
    {
        "key": "ctrl+tab",
        "command": "editor.action.indentLines",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+tab",
        "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
        "when": "!activeEditorGroupEmpty"
    },
    {
        "key": "ctrl+tab",
        "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
        "when": "inEditorsPicker && inQuickOpen"
    },
    {
        "key": "ctrl+enter",
        "command": "-github.copilot.generate",
        "when": "editorTextFocus && github.copilot.activated && !commentEditorFocused && !inInteractiveInput && !interactiveEditorFocused"
    },
    {
        "key": "ctrl+alt+[Minus]",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+alt+d",
        "command": "editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "-editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+i",
        "command": "inlineChat.start",
        "when": "inlineChatHasEditsAgent && inlineChatPossible && !editorReadonly && !editorSimpleInput || inlineChatHasProvider && inlineChatPossible && !editorReadonly && !editorSimpleInput"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "search-preview.quickOpenWithPreview"
    },
    {
        "key": "ctrl+alt+z",
        "command": "workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "ctrl+k z",
        "command": "-workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.emmet.action.decrementNumberByOne"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.emmet.action.incrementNumberByOne"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-markdown.showPreview",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+alt+v",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    }
]